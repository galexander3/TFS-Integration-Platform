<UserControl x:Class="Microsoft.TeamFoundation.Migration.Shell.TfsCommonShellAdapter.ListPathsControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008">
    <Grid>
        <ListBox HorizontalAlignment="Left" Margin="6,10,0,0" Name="listBox1" VerticalAlignment="Top" MinWidth="200" MaxHeight="120"
                 ItemsSource="{Binding AllPaths}" SelectedItem="{Binding SelectedPath}" Padding="0,3,3,3"/>
    </Grid>
</UserControl>
