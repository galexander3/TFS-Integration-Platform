<StackPanel x:Class="Microsoft.TeamFoundation.Migration.Shell.View.DefaultAbout"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <StackPanel Name="iconInfoStackPanel" Style="{DynamicResource IconInfoStackPanelStyle}">
        
        <Image Name="applicationIcon" Style="{DynamicResource ApplicationIconStyle}" />

        <StackPanel Name="infoStackPanel" Style="{DynamicResource InfoStackPanelStyle}">
            <TextBlock Name="applicationNameTextBlock" Style="{DynamicResource ApplicationNameTextBlockStyle}" />
            <TextBlock Name="applicationVersionTextBlock" Style="{DynamicResource ApplicationVersionTextBlockStyle}" />
            <TextBlock Name="copyrightTextBlock" Style="{DynamicResource CopyrightTextBlockStyle}" />
        </StackPanel>

    </StackPanel>
    
    <Rectangle Name="aboutDividerRectangle" Style="{DynamicResource AboutDividerRectangleStyle}" />

    <DockPanel Name="copyrightWarningDockPanel" Style="{DynamicResource CopyrightWarningDockPanelStyle}">
        <TextBlock Name="copyrightWarningTextBlock" Style="{DynamicResource CopyrightWarningTextBlockStyle}" />
        <Button Name="aboutOkButton" Style="{DynamicResource AboutOkButtonStyle}" />
    </DockPanel>

</StackPanel>
