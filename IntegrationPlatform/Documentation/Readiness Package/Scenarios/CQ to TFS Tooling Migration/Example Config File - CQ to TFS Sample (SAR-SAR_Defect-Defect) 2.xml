                  <Value LeftValue="4" RightValue="4 - Low: inconvenience"></Value>
                  <Value LeftValue="5" RightValue="5 - All other errors"></Value>
                  <Value LeftValue="E" RightValue="E - SPR Emergency"></Value>
                  <Value LeftValue="R" RightValue="R - SPR Routine"></Value>
                  <Value LeftValue="U" RightValue="U - SPR Urgent"></Value>
                </ValueMap>
                <ValueMap name="PriorityMap">
                  <Value LeftValue="1" RightValue="1"></Value>
                  <Value LeftValue="2" RightValue="2"></Value>
                  <Value LeftValue="3" RightValue="3"></Value>
                  <Value LeftValue="4" RightValue="4"></Value>
                  <Value LeftValue="5" RightValue="5"></Value>
                  <Value LeftValue="E" RightValue="6"></Value>
                  <Value LeftValue="R" RightValue="7"></Value>
                  <Value LeftValue="U" RightValue="8"></Value>
                </ValueMap>
                <ValueMap name="OriginMap">
                  <Value LeftValue="Internal" RightValue="Internal"></Value>
                  <Value LeftValue="External" RightValue="Customer"></Value>
                  <Value LeftValue="*" RightValue="Internal"></Value>
                </ValueMap>
              </ValueMaps>
            </WITSessionCustomSetting>
          </SettingXml>
        </CustomSettings>
      </Session>
      <!--Defect Migration Session-->
      <Session SessionUniqueId="882d715f-214a-4901-aefb-a309ed4a8bd7" 
               FriendlyName="Defect Session" 
               LeftMigrationSourceUniqueId="4F0865E8-5519-4440-8CBB-DDE0EA5D3E0A" 
               RightMigrationSourceUniqueId="97CC433E-CB2E-4340-8578-6CE070CE9ECF" 
               SessionType="WorkItemTracking">
        <EventSinks />
        <Filters>
          <FilterPair>
            <FilterItem MigrationSourceUniqueId="4F0865E8-5519-4440-8CBB-DDE0EA5D3E0A" 
                        FilterString="SAR::defect_1::(dbid &lt;&gt; 0 and form_planned_release = '##RELEASE##' and state in (select id from statedef T2 where T2.name in ('Approved', 'Built', 'Entered_Test', 'Failed', 'Integration_Failed', 'Integration_Passed', 'MergedToInteg', 'MergedToProd', 'Opened', 'Passed', 'Resolved')) and product in (select dbid from product T3 where T3.name = '##NAME##'))" />
            <FilterItem MigrationSourceUniqueId="97CC433E-CB2E-4340-8578-6CE070CE9ECF" 
                        FilterString="[System.Id] = 0" />
          </FilterPair>
        </Filters>
        <CustomSettings>
          <SettingXml>
            <WITSessionCustomSetting>
              <WorkItemTypes>
                <WorkItemType LeftWorkItemTypeName="SAR" RightWorkItemTypeName="Defect" fieldMap="SARtoDefect" />
              </WorkItemTypes>
              <FieldMaps>
                <FieldMap name="SARtoDefect">
                  <MappedFields>
                    <MappedField LeftName="@@MissingField@@" RightName="FeatureModel" MapFromSide="Left" valueMap="FeatureModelMap" />
                    <MappedField LeftName="sar_type" RightName="SarType" MapFromSide="Left" valueMap="" />
                    <MappedField LeftName="root_cause" RightName="Microsoft.VSTS.CMMI.RootCause" MapFromSide="Left" valueMap="RootCauseMap"></MappedField>
                    <MappedField LeftName="root_cause_other" RightName="OtherRootCause" MapFromSide="Left" valueMap=""></MappedField>
                    <MappedField LeftName="defect_catagory" RightName="PhaseInjected" MapFromSide="Left" valueMap="" />
                    <MappedField LeftName="phase_detected" RightName="PhaseDetected" MapFromSide="Left" valueMap="" />
                    <MappedField LeftName="assigned_to_username" RightName="System.AssignedTo" MapFromSide="Left" valueMap="AssignedToMap" />
                    <MappedField LeftName="State" RightName="System.State" MapFromSide="Left" valueMap="DefectStateMap" />
                    <MappedField LeftName="resolution" RightName="System.Reason" MapFromSide="Left" valueMap="DefectReasonMap" />
                    <!--Using submit_date to as a temporary source for resolved date since resolved date is not availble during this time.-->
                    <MappedField LeftName="submit_date" RightName="Microsoft.VSTS.Common.ResolvedDate" MapFromSide="Left" valueMap="ResolvedDateMap"></MappedField>
                  </MappedFields>
                  <AggregatedFields>
                    <FieldsAggregationGroup MapFromSide="Left" TargetFieldName="System.Title" Format="CSC:{0} {1}">
                      <SourceField Index="0" SourceFieldName="form_csc_id"></SourceField>
                      <SourceField Index="1" SourceFieldName="csc_subfunction" />
                    </FieldsAggregationGroup>
                  </AggregatedFields>
                  <UserIdentityFields>
                    <LeftUserIdentityFields>
                      <UserIdField FieldReferenceName="assigned_to_username" UserIdPropertyName="Alias" />
                    </LeftUserIdentityFields>
                    <RightUserIdentityFields>
                      <UserIdField FieldReferenceName="System.AssignedTo" UserIdPropertyName="DisplayName" />
                    </RightUserIdentityFields>
                  </UserIdentityFields>
                </FieldMap>
              </FieldMaps>
              <ValueMaps>
                <ValueMap name="FeatureModelMap">
                  <Value LeftValue="" RightValue="-"></Value>
                </ValueMap>
                <ValueMap name="RootCauseMap">
                  <Value LeftValue="Arch/Design Shortfal" RightValue="Arch/Design Shortfall"></Value>
                  <Value LeftValue="Code/Doc Style" RightValue="Code/Doc Style"></Value>
                  <Value LeftValue="Code Logic Error" RightValue="Code Logic Error"></Value>
                  <Value LeftValue="Code Typo" RightValue="Code Typo"></Value>
                  <Value LeftValue="Documentation Error" RightValue="Documentation Error"></Value>
                  <Value LeftValue="Duplicate" RightValue="Duplicate"></Value>
                  <Value LeftValue="FAD" RightValue="FAD"></Value>
                  <Value LeftValue="Installer Ambiguous Specification" RightValue="Installer Ambiguous Specification"></Value>
                  <Value LeftValue="Installer Implementation Error" RightValue="Installer Implementation Error"></Value>
                  <Value LeftValue="Installer New Feature Development" RightValue="Installer New Feature Development"></Value>
                  <Value LeftValue="Installer Specification Update" RightValue="Installer Specification Update"></Value>
                  <Value LeftValue="Merge Error" RightValue="Merge Error"></Value>
                  <Value LeftValue="Missing Functionality" RightValue="Missing Functionality"></Value>
                  <Value LeftValue="Other" RightValue="Other"></Value>
                  <Value LeftValue="Planning SAR" RightValue="Planning SAR"></Value>
                  <Value LeftValue="Reference Error" RightValue="Reference Error"></Value>
                  <Value LeftValue="Requirements Change" RightValue="Requirements Change"></Value>
                  <Value LeftValue="Requirements Completeness" RightValue="Requirements Completeness"></Value>
                  <Value LeftValue="Requirements Deferred" RightValue="Requirements Deferred"></Value>
                  <Value LeftValue="Requirements Incorrect" RightValue="Requirements Incorrect"></Value>
                  <Value LeftValue="Requirements Logic" RightValue="Requirements Logic"></Value>
                  <Value LeftValue="Requirements Presentation" RightValue="Requirements Presentation"></Value>
                  <Value LeftValue="Requirments Verifiability" RightValue="Requirments Verifiability"></Value>
                  <Value LeftValue="Test Case Incorrect/Ambiguous" RightValue="Test Case Incorrect/Ambiguous"></Value>
                  <Value LeftValue="Reference Error" RightValue="Ref Error"></Value>
                  <Value LeftValue="Un-Reproducible" RightValue="Un-Reproducible"></Value>
                  <Value LeftValue="*" RightValue="Unknown"></Value>
                </ValueMap>
                <ValueMap name="AssignedToMap">
                  <Value LeftValue="*" RightValue="">
                    <When ConditionalSrcFieldName="State" ConditionalSrcFieldValue="Submitted"></When>
                  </Value>
                </ValueMap>
                <ValueMap name="DefectStateMap">
                  <!--Default mapping for when none of the following conditions are met-->
                  <Value LeftValue="*" RightValue="Active" />
                  <!--Opened-->
                  <Value LeftValue="Opened" RightValue="Resolved">
                    <When ConditionalSrcFieldName="resolution" ConditionalSrcFieldValue="Duplicate"></When>
                  </Value>
                  <Value LeftValue="Opened" RightValue="Resolved">
                    <When ConditionalSrcFieldName="resolution" ConditionalSrcFieldValue="Functions as Designed"></When>
                  </Value>
                  <Value LeftValue="Opened" RightValue="Active"></Value>
                  <!--Resolved-->
                  <Value LeftValue="Resolved" RightValue="Resolved" />
                  <!--MergedToInteg-->
                  <Value LeftValue="MergedToInteg" RightValue="Resolved" />
                  <!--MergedToProd-->
                  <Value LeftValue="MergedToProd" RightValue="Resolved" />
                  <!--Built-->
                  <Value LeftValue="Built" RightValue="Resolved" />
                  <!--Integration_Passed-->
                  <Value LeftValue="Integration_Passed" RightValue="Resolved" />
                  <!--Integration_Failed-->
                  <Value LeftValue="Integration_Failed" RightValue="Resolved">
                    <When ConditionalSrcFieldName="resolution" ConditionalSrcFieldValue="Duplicate"></When>
                  </Value>
                  <Value LeftValue="Integration_Failed" RightValue="Resolved">
                    <When ConditionalSrcFieldName="resolution" ConditionalSrcFieldValue="Functions as Designed"></When>
                  </Value>
                  <Value LeftValue="Integration_Failed" RightValue="Active" />
                  <!--Approved-->
                  <Value LeftValue="Approved" RightValue="Resolved" />
                  <!--Entered_Test-->
                  <Value LeftValue="Entered_Test" RightValue="Resolved" />
                  <!--Passed-->
                  <Value LeftValue="Passed" RightValue="Resolved" />
                  <!--Failed-->
                  <Value LeftValue="Failed" RightValue="Resolved">
                    <When ConditionalSrcFieldName="resolution" ConditionalSrcFieldValue="Duplicate"></When>
                  </Value>
                  <Value LeftValue="Failed" RightValue="Resolved">
                    <When ConditionalSrcFieldName="resolution" ConditionalSrcFieldValue="Functions as Designed"></When>
                  </Value>
                  <Value LeftValue="Failed" RightValue="Active" />
                </ValueMap>
                <ValueMap name="DefectReasonMap">
                  <!--Default mapping for when none of the following conditions are met-->
                  <Value LeftValue="*" RightValue="New"></Value>
                  <!--No resolution value - possible only when CQ State = Opened-->
                  <Value LeftValue="" RightValue="New">
                    <!--CQ State = Opened-->
                    <When ConditionalSrcFieldName="State" ConditionalSrcFieldValue="Opened" />
                  </Value>
                  <!--Duplicate-->
                  <Value LeftValue="Duplicate" RightValue="Resolved"></Value>
                  <!--Functions as Designed-->
                  <Value LeftValue="Functions as Designed" RightValue="Resolved"></Value>
                  <!--Enhancement Request-->
                  <Value LeftValue="Enhancement Request" RightValue="New">
                    <!--CQ State = Opened-->
                    <When ConditionalSrcFieldName="State" ConditionalSrcFieldValue="Opened" />
                  </Value>
                  <Value LeftValue="Enhancement Request" RightValue="Reactivated">
                    <!--CQ State = Integration_Failed-->
                    <When ConditionalSrcFieldName="State" ConditionalSrcFieldValue="Integration_Failed" />
                  </Value>
                  <Value LeftValue="Enhancement Request" RightValue="Reactivated">
                    <!--CQ State = Failed-->
                    <When ConditionalSrcFieldName="State" ConditionalSrcFieldValue="Failed" />
                  </Value>
                  <Value LeftValue="Enhancement Request" RightValue="Resolved"></Value>
                  <!--Fixed-->
                  <Value LeftValue="Fixed" RightValue="New">
                    <!--CQ State = Opened-->
                    <When ConditionalSrcFieldName="State" ConditionalSrcFieldValue="Opened"></When>
                  </Value>
                  <Value LeftValue="Fixed" RightValue="Reactivated">
                    <!--CQ State = Integration_Failed-->
                    <When ConditionalSrcFieldName="State" ConditionalSrcFieldValue="Integration_Failed" />
                  </Value>
                  <Value LeftValue="Fixed" RightValue="Reactivated">
                    <!--CQ State = Failed-->
                    <When ConditionalSrcFieldName="State" ConditionalSrcFieldValue="Failed" />
                  </Value>
                  <Value LeftValue="Fixed" RightValue="Resolved"></Value>
                  <!--Fixed - Indirectly-->
                  <Value LeftValue="Fixed - Indirectly" RightValue="New">
                    <!--CQ State = Opened-->
                    <When ConditionalSrcFieldName="State" ConditionalSrcFieldValue="Opened" />
                  </Value>
                  <Value LeftValue="Fixed - Indirectly" RightValue="Reactivated">
                    <!--CQ State = Integration_Failed-->
                    <When ConditionalSrcFieldName="State" ConditionalSrcFieldValue="Integration_Failed" />
                  </Value>
                  <Value LeftValue="Fixed - Indirectly" RightValue="Reactivated">
                    <!--CQ State = Failed-->
                    <When ConditionalSrcFieldName="State" ConditionalSrcFieldValue="Failed" />
                  </Value>
                  <Value LeftValue="Fixed - Indirectly" RightValue="Resolved"></Value>
                </ValueMap>
                <ValueMap name="ResolvedDateMap">
                  <!--When none of the conditions are met, the default mapping sets the resolved date to empty. Otherwise, the TFS resolved date is populated from the resolved date in CQ (once available) -->
                  <Value LeftValue="*" RightValue=""></Value>
                  <!--CQ Resolution is duplicate or functions as designed-->
                  <Value LeftValue="*" RightValue="*">
                    <When ConditionalSrcFieldName="resolution" ConditionalSrcFieldValue="Duplicate"></When>
                  </Value>
                  <Value LeftValue="*" RightValue="*">
                    <When ConditionalSrcFieldName="resolution" ConditionalSrcFieldValue="Functions as Designed"></When>
                  </Value>
                  <!--CQ State is Resolved-->
                  <Value LeftValue="*" RightValue="*">
                    <When ConditionalSrcFieldName="State" ConditionalSrcFieldValue="Resolved"></When>
                  </Value>
                  <!--CQ State is MergedToInteg-->
                  <Value LeftValue="*" RightValue="*">
                    <When ConditionalSrcFieldName="State" ConditionalSrcFieldValue="MergedToInteg"></When>
                  </Value>
                  <!--CQ State is MergedToProd-->
                  <Value LeftValue="*" RightValue="*">
                    <When ConditionalSrcFieldName="State" ConditionalSrcFieldValue="MergedToProd"></When>
                  </Value>
                  <!--CQ State is Built-->
                  <Value LeftValue="*" RightValue="*">
                    <When ConditionalSrcFieldName="State" ConditionalSrcFieldValue="Built"></When>
                  </Value>
                  <!--CQ State is Integration_Passed-->
                  <Value LeftValue="*" RightValue="*">
                    <When ConditionalSrcFieldName="State" ConditionalSrcFieldValue="Integration_Passed"></When>
                  </Value>
                  <!--CQ State is Approved-->
                  <Value LeftValue="*" RightValue="*">
                    <When ConditionalSrcFieldName="State" ConditionalSrcFieldValue="Approved"></When>
                  </Value>
                  <!--CQ State is Entered_Test-->
                  <Value LeftValue="*" RightValue="*">
                    <When ConditionalSrcFieldName="State" ConditionalSrcFieldValue="Entered_Test"></When>
                  </Value>
                  <!--CQ State is Passed-->
                  <Value LeftValue="*" RightValue="*">
                    <When ConditionalSrcFieldName="State" ConditionalSrcFieldValue="Passed"></When>
                  </Value>
                </ValueMap>
              </ValueMaps>
            </WITSessionCustomSetting>
          </SettingXml>
        </CustomSettings>
      </Session>
    </Sessions>
    <Linking>
      <CustomSettings></CustomSettings>
      <LinkTypeMappings>
        <LinkTypeMapping LeftMigrationSourceUniqueId="BB2BD2C6-92B5-4817-AB51-A087B6532F0D" 
                         LeftLinkType="ClearQuestAdapter.LinkType.Web.RecordHyperLink" 
                         RightMigrationSourceUniqueId="F4741818-F14C-458F-BB20-4BBAC20F95C3" 
                         RightLinkType="Microsoft.TeamFoundation.Migration.TFS.LinkType.WorkItemToHyperlink" />
      </LinkTypeMappings>
    </Linking>
  </SessionGroup>
</Configuration>